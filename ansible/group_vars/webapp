---
nginx_sites:
  - server_name: league.internal.codingstones.com
    type: upstream
    listen: 80
    upstreams:
      - 'localhost:8080'
    locations:
      - url: /
        directives:
          - proxy_set_header: Host $host
          - proxy_set_header: X-Forwarded-Server $host
          - proxy_set_header: X-Forwarded-For $proxy_add_x_forwarded_for
          - proxy_pass: http://backend

